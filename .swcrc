{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "dynamicImport": true,
            "decorators": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "experimental": {
            "keepImportAssertions": true
        },
        "baseUrl": ".",
        "paths": {},
        "target": "es2021"
    },
    "module": {
      "type": "es6"
    },
    "minify": true
}